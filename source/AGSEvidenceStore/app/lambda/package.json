    "scripts": {
        "preinstall": "npx force-resolutions",
        "lint": "eslint --ext .ts . --fix && prettier --write './**/*.{ts,js,json}' ",
        "build": "npm run lint && webpack && webpack --config stream-processor-webpack.config.js && webpack --config s3-evidence-collector-webpack.config.js && npm run test",
        "build:doc": "npx apidoc -i ./src && npx apidoc-markdown -p doc/ -o API.md",
        "test": "jest unit --silent",
        "test:watch": "jest unit --coverage=false --watch",
        "integ-test": "jest --roots test/integration --detectOpenHandles --config integ.jest.config.js",
        "start:local": "sam local start-api",
        "all": "npm run build"
    },
    "apidoc": {
        "name": "AGS Evidence Store",
        "description": "AGS Evidence Store API Documentation",
        "sampleUrl": "https://<Service Endpoint>",
        "version": "1.0.0"
    },
    "resolutions": {
        "async": "2.6.4",
        "got": "11.8.5"
