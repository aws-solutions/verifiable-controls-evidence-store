{
    "name": "security-hub-evidence-collector-infra",
    "version": "1.2.0",
    "bin": {
        "infra": "bin/infra.js"
    },
    "scripts": {
        "lint": "eslint --ext .ts . --fix && prettier --write './**/*.{ts,js,json}' ",
        "build": "tsc && cd lib/evidence-store-onboarder && npm run build && cd ../..",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "synth": "cdk context --clear && cdk synth -q",
        "all": "npm run lint && npm run build && npm run synth",
        "deploy": "cd lib/evidence-store-onboarder && npm run build && cd ../.. && npm run synth && cdk deploy -a cdk.out/assembly-PersonalStage"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/js-yaml": "^4.0.0",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "aws-cdk": "^2.20.0",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-typescript": "^3.0.0",
        "eslint-plugin-header": "^3.1.1",
        "jest": "^27.3.1",
        "jest-junit": "^13.0.0",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@ags-cdk/ags-service-template": "^3.0.0-alpha.7",
        "@ags-cdk/ags-solution-metrics": "^2.0.0-alpha.0",
        "@ags-cdk/ags-synthetics-canary": "^3.0.0-alpha.0",
        "@aws-cdk/aws-synthetics-alpha": "^2.20.0-alpha.0",
        "constructs": "^10.0.119",
        "js-yaml": "^4.0.0",
        "source-map-support": "^0.5.16"
    }
}
